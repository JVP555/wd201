<!DOCTYPE html>

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <link href="./css/style.css" rel="stylesheet" />
    </head>
    <body>
        <%- include('header.ejs') %>
        <table>
            <% for (var i = 0; i < allTodos.length; i++) { %>
            <tr>
                <td>
                <%= allTodos[i].title %>
                </td>
                <td>
                <%= allTodos[i].dueDate %>
                </td>
                <td>
                <%= allTodos[i].completed %>
                </td>
            </tr>
            <% } %>
        </table>
    </body>
</html>